{{#each ironhackersAll}}
<!-- Team member -->
<div class="col-xs-12 col-sm-6 col-md-4">
  <div class="image-flip" ontouchstart="this.classList.toggle('hover');">
    <div class="mainflip">
      <div class="frontside">
        <div class="card">
          <div class="card-body text-center">
            <p><img class="img-fluid" src="{{this.image}}" alt="card image" /></p>
            <h4 class="card-title">{{this.firstname}} {{this.lastname}}</h4>
            <p class="card-text">{{this.resume}}</p>
            <a href="#" class="btn btn-primary btn-sm"><i class="fa fa-plus"></i></a>
          </div>
        </div>
      </div>
      <div class="backside">
        <div class="card">
          <div class="card-body text-center">
            <h4 class="card-title">{{this.firstname}} {{this.lastname}}</h4>
            {{#each skills}}
            <ul class="fa-ul">
            <li><i class="fa-li fa fa-check-square"></i>{{this.name}}</li>
            </ul>
            {{/each}}
              <a class="social-icon text-center btn" target="_blank" href="{{this.linkedinProfile}}">
                <i class="fa fa-linkedin"></i>
              </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
{{/each}}